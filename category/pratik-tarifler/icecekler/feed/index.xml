	<item>
		<title>Limonlu Soğuk Çay</title>
		<link>/limonlu-soguk-cay/</link>
					<comments>/limonlu-soguk-cay/#respond</comments>
		
		<dc:creator><![CDATA[sozmutfakta]]></dc:creator>
		<pubDate>Tue, 25 Jun 2013 04:21:42 +0000</pubDate>
				<category><![CDATA[İçecekler]]></category>
